function TestImageInput(img: image) -> string{
  client GPT4o
  prompt #"
    {{ _.role("user") }}

    Describe this in 4 words. One word must be the color {{img}}
  "#
}

test  TestImageInput {
  functions [TestImageInput]
  args {
    img {
      media_type "image/png"
      // url gs://cloud-samples-data/vertex-ai/llm/prompts/landmark1.png

      url "https://www.google.com/images/branding/googlelogo/2x/googlelogo_color_92x30dp.png"
    }
  }
}

test  shrek {
  functions [TestImageInput]
  args {
    img {
      
      url "https://upload.wikimedia.org/wikipedia/en/4/4d/Shrek_%28character%29.png"
    }
  }
}
test  ashrek2 {
  functions [TestImageInput]
  args {
    img {
      media_type "image/png"
      // file "baml_src://shrek.png"
      file "shrek .png"
      //file "shrek-ðŸµ.png"
    }
  }
}



