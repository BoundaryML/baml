{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "module": "commonjs",
    "noUnusedLocals": false,
    "target": "es6",
    "outDir": "out",
    "rootDir": "src",
    "lib": ["es6"],
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "composite": true,
    "declaration": true,
    "skipLibCheck": true,
    "noUnusedParameters": false
  },
  "include": ["src"],
  "exclude": ["node_modules", ".vscode-test"]
}
